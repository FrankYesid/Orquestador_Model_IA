# Configuración de rutas del proyecto
PROJECT_ROOT=d:/GitHub/Orquestador_Model_IA
DATA_INPUT_PATH=d:/GitHub/Orquestador_Model_IA/data/input
DATA_OUTPUT_PATH=d:/GitHub/Orquestador_Model_IA/data/output
DB_PATH=d:/GitHub/Orquestador_Model_IA/data/database.db

# Configuración de Airflow
AIRFLOW_HOME=d:/GitHub/Orquestador_Model_IA/airflow
AIRFLOW__CORE__DAGS_FOLDER=d:/GitHub/Orquestador_Model_IA/dags
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=sqlite:///d:/GitHub/Orquestador_Model_IA/airflow/airflow.db

# Configuración de la base de datos
DB_TYPE=sqlite
